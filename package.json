{
	"name": "@obsidize/rx-console",
	"version": "6.0.0",
	"description": "A logging library that is tiny, highly configurable, and platform-agnostic",
	"main": "es5.js",
	"module": "index.js",
	"types": "index.d.ts",
	"typings": "index.d.ts",
	"scripts": {
		"test": "karma start",
		"test:single": "karma start --single-run",
		"build:webpack": "webpack --config webpack.config.js",
		"build:tsc": "tsc",
		"build": "run-s build:tsc build:webpack copy:assets",
		"clean": "rimraf ./dist ./coverage",
		"rebuild": "run-s clean build",
		"coverage:open": "open-cli ./coverage/*/html/index.html",
		"coverage": "run-s test:single coverage:open",
		"copy:package": "cpy ./package.json ./dist/",
		"copy:readme": "cpy ./README.md ./dist/",
		"copy:assets": "run-p copy:package copy:readme",
		"git:publish": "node ./scripts/release-publish.js",
		"dist:publish": "npm publish ./dist",
		"dist:pack": "node ./scripts/release-pack.js",
		"dist:bundle": "run-s rebuild coverage docs",
		"version": "npm run dist:bundle",
		"version:publish": "run-s git:publish dist:publish",
		"version:patch": "npm version patch",
		"bump": "run-s version:patch version:publish",
		"docs": "typedoc ./src/index.ts --out ./docs --excludePrivate",
		"docs:open": "open-cli ./docs/index.html"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jospete/obsidize-rx-console.git"
	},
	"keywords": [
		"log",
		"logger",
		"console",
		"rx",
		"rxjs",
		"obsidize"
	],
	"author": "Josh Noel <obsidize@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jospete/obsidize-rx-console/issues"
	},
	"homepage": "https://github.com/jospete/obsidize-rx-console#readme",
	"devDependencies": {
		"@types/fs-extra": "9.0.13",
		"@types/jasmine": "4.3.1",
		"cpy-cli": "^4.2.0",
		"fs-extra": "10.0.1",
		"jasmine-core": "4.5.0",
		"karma": "6.4.1",
		"karma-chrome-launcher": "3.1.1",
		"karma-cli": "2.0.0",
		"karma-coverage": "2.2.0",
		"karma-coverage-istanbul-reporter": "3.0.3",
		"karma-jasmine": "5.1.0",
		"karma-jasmine-html-reporter": "2.0.0",
		"karma-typescript": "5.5.3",
		"npm-run-all": "4.1.5",
		"open-cli": "7.1.0",
		"rimraf": "^3.0.2",
		"rxjs": "7.8.0",
		"terser-webpack-plugin": "5.3.6",
		"ts-loader": "9.4.2",
		"typedoc": "0.23.24",
		"typescript": "4.9.4",
		"webpack": "5.75.0",
		"webpack-cli": "5.0.1",
		"yargs": "17.6.2"
	}
}
